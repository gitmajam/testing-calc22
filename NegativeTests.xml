<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="NegativeLoginTestSuite" verbose="1"
	data-provider-thread-count="5">
	<listeners>
		<listener
			class-name="com.tribu.qaselenium.testframework.testbase.EmailableTestingReporter" />
	</listeners>
	<test name="NegativeLoginTest-CH">
		<parameter name="browser" value="chrome"></parameter>
		<classes>
			<class
				name="com.tribu.qaselenium.tests.negative.NegativeTests">
			</class>
		</classes>
	</test>
</suite>
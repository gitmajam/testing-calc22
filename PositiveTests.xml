		/> <class name="com.tribu.qaselenium.tests.mazaprendo.MACourseCertificateTests" 
		/> </classes> </test> -->

	<test name="lifecycleF2FCourse" preserve-order="true">
		<parameter name="browser" value="chrome"></parameter>
		<groups>
			<run>
				<include name="smoke" />
			</run>
		</groups>
		<classes>
			<class
				name="com.tribu.qaselenium.tests.mazaprendo.MAUploadEvidenceTests" />
			<class
				name="com.tribu.qaselenium.tests.mazaprendo.MACheckCourseEvidenceTests" />
			<class
				name="com.tribu.qaselenium.tests.mazaprendo.MAFeedbackCourseTests" />
			<class
				name="com.tribu.qaselenium.tests.mazaprendo.MAScoreCourseTests" />
	<!-- 		<class
				name="com.tribu.qaselenium.tests.mazaprendo.MACourseCertificateTests" /> -->
		</classes>
	</test>


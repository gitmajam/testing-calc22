<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="PositiveLoginTestSuite" verbose="5"
	data-provider-thread-count="5">
	<listeners>
		<listener
			class-name="com.tribu.qaselenium.testframework.testbase.EmailableTestingReporter" />
	</listeners>
	<test name="MACourseLifecycle" preserve-order="true"> <!-- parallel = "methods" -->
		<parameter name="browser" value="chrome"></parameter>
		<groups>
			<run>
				<include name="smoke" />
			</run>
		</groups>
		<classes>
			<class
				name="com.tribu.qaselenium.tests.mazaprendo.MAClearCacheTests" />
			<class
				name="com.tribu.qaselenium.tests.mazaprendo.MACreateCourseTests" />
			<class
				name="com.tribu.qaselenium.tests.mazaprendo.MAAssingCourseTests" />
			<class
				name="com.tribu.qaselenium.tests.mazaprendo.MAPerformCourseTests" />
			<class name="com.tribu.qaselenium.tests.mazaprendo.MACourseCertificateTests" 
				/>
			<class
				name="com.tribu.qaselenium.tests.mazaprendo.MADeleteCourseTests" />

		</classes>
	</test>
	<!-- <test name="MALessonsAttempts" preserve-order="true"> parallel = "methods" 
		<parameter name="browser" value="chrome"></parameter> <groups> <run> <include 
		name="smoke" /> </run> </groups> <classes> <class name="com.tribu.qaselenium.tests.mazaprendo.MAAssingCourseTests" 
		/> <class name="com.tribu.qaselenium.tests.mazaprendo.MALessonAttemptTests" 
		/> <class name="com.tribu.qaselenium.tests.mazaprendo.MADeleteCourseTests" 
		/> </classes> </test> -->
</suite>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="PositiveLoginTestSuite" verbose="5"
	data-provider-thread-count="5">

	<listeners>
		<listener
			class-name="com.tribu.qaselenium.testframework.testbase.EmailableTestingReporter" />
	</listeners>
	<test name="apiClearCache" preserve-order="true">
		<parameter name="browser" value="chrome"></parameter>
		<classes>
			<class
				name="com.tribu.qaselenium.tests.apimazaprendo.MAClearCacheTests" />
		</classes>
	</test>
	<test name="preconditions" preserve-order="true">
		<parameter name="browser" value="chrome"></parameter>
		<classes>
			<class
				name="com.tribu.qaselenium.tests.apimazaprendo.MAPreconditionTests" />
		</classes>
	</test>

	<!-- <test name="apiCreateDigitalCourse" preserve-order="true"> <parameter 

		/> <class name="com.tribu.qaselenium.tests.apimazaprendo.MACreateDigitalLessonTests" 
		/> <class name="com.tribu.qaselenium.tests.apimazaprendo.MAAssignDigitalCourseTests" 
		/> </classes> </test> -->

	<test name="apiCreateF2FCourse" preserve-order="true">
		<parameter name="browser" value="chrome"></parameter>
		<classes>
			<class
				name="com.tribu.qaselenium.tests.apimazaprendo.MACreateF2FCourseTests" />
			<class
				name="com.tribu.qaselenium.tests.apimazaprendo.MACreateF2FLessonTests" />
			<class
				name="com.tribu.qaselenium.tests.apimazaprendo.MAAssignF2FCourseTests" />
		</classes>
	</test>

	<!-- <test name="lifecycleDigitalCourse" preserve-order="true"> <parameter 

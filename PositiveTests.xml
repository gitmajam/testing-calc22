<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="PositiveLoginTestSuite" verbose="5"
	data-provider-thread-count="5">
	<listeners>
		<listener class-name="com.tribu.seleniumframework.testbase.EmailableTestingReporter" />
	</listeners>
	<test name="PositiveLoginTest-CH">
		<parameter name="browser" value="chrome"></parameter>
		<classes>
			<class
				name="com.tribu.seleniumframework.tests.PositiveLoginTests" />
		</classes>
	</test>
</suite>